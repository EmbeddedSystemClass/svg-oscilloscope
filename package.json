{
  "name": "vdom-oscilloscope",
  "version": "0.0.0",
  "description": "render ndsamples as virtual svgs",
  "main": "index.js",
  "scripts": {
    "start": "budo example.js --serve bundle.js --live",
    "build": "browserify example.js -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahdinosaur/vdom-oscilloscope"
  },
  "author": "ahdinosaur",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahdinosaur/vdom-oscilloscope/issues"
  },
  "homepage": "https://github.com/ahdinosaur/vdom-oscilloscope",
  "dependencies": {
    "defined": "^1.0.0",
    "ndarray": "^1.0.18",
    "virtual-hyperscript-svg": "^2.0.0"
  },
  "devDependencies": {
    "CBuffer": "^1.1.0",
    "babelify": "^6.1.3",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "main-loop": "^3.1.0",
    "next-tick": "^0.2.2",
    "read-audio": "^6.1.1",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "virtual-dom": "^2.0.1",
    "watchify": "^3.1.0",
    "writable2": "^0.1.2"
  }
}
